<form class="new-card">
  <input type="hidden" name="card[list_id]" class="list-id" value="<%= listId %>">
  <label for="title">Title</label>
  <input type="text" id="title" name="card[title]" class="card-title">
  <br>
  <label for="description">Description</label>
  <input type="text" id="description" name="card[description]" class="card-description">
  <button class="add-card">Add</button>
</form>