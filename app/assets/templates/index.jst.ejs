<h1 class="board-title">Ur Burds</h1>
<% boards.each(function(board) { %>
  <li class="board-el">
    <a href="#/boards/<%= board.escape("id") %>">
      <%= board.get("title") %>
    </a>
    <button class="delete-board" id="<%= board.escape("id") %>">
      DELETE
    </button>
  </li>
<% }); %>
<a href="#/boards/new" class="new-board-link">
  Create new burd
</a>